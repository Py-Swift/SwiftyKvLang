<CompleteExample@BoxLayout>:
    orientation: 'vertical'
    spacing: 10
    on_touch_down: print("Layout touched!")
    
    Label:
        text: 'Event Handler Demo'
        font_size: 24
    
    Button:
        text: 'Button 1'
        on_press: app.handle_button1()
    
    Button:
        text: 'Button 2'
        on_release: print("Button 2 released!")
    
    Switch:
        id: my_switch
        on_active: app.switch_changed(self.active)
